{
  "extends": ["airbnb", "prettier", "react-app"],
  "plugins": ["prettier"],
  "rules": {
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "prettier/prettier": ["error"],
    "jsx-a11y/href-no-hash": [0],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["to"]
      }
    ]
  }
}
